{
  "name": "monorepo-example",
  "private": true,
  "packageManager": "yarn@3.3.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:nextjs": "turbo run dev --filter=nextjs",
    "start:storybook": "turbo run start --filter=storybook",
    "storybook": "turbo run storybook --filter=storybook"
  },
  "devDependencies": {
    "turbo": "^1.6.3"
  }
}
